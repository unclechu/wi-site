{
  "dependencies": {
    "front-end-gulp-pattern": "^1",

    "gulp-browserify": "~0.5",
    "gulp-uglify": "^1.0.1",
    "LiveScript": "1.3.1",
    "liveify-stable": "^1",
    "prelude-ls": "1.1.1",

    "gulp-stylus": "^1.3.4",
    "nib": "^1.0.4",

    "bower": "^1"
  },
  "scripts": {
    "postinstall": "./deploy.sh"
  },
  "gulp": {
    "distclean": [
      "./front-end-gulp",
      "./node_modules",
      "./bower_components"
    ],
    "scripts": {
      "main": {
        "type": "browserify",
        "path": "./scripts/",
        "mainSrc": "main.ls",
        "buildFile": "build.js",
        "transform": ["liveify-stable"],
        "extensions": [".ls"],
        "jshintDisabled": true,
        "shim": {
          "prelude": {
            "path": "./node_modules/prelude-ls/",
            "exports": ""
          },
          "jquery": {
            "path": "./bower_components/jquery/dist/jquery.min.js",
            "exports": ""
          },
          "jquery.transit": {
            "path": "./bower_components/jquery.transit/jquery.transit.js",
            "exports": ""
          },
          "modernizr": {
            "path": "./bower_components/modernizr/modernizr.js",
            "exports": "Modernizr"
          },
          "jquery.timers": {
            "path": "./scripts/src/lib/jquery.timers.js",
            "exports": "",
            "depends": {
              "jquery": "jQuery"
            }
          }
        }
      }
    },
    "styles": {
      "main": {
        "type": "stylus",
        "path": "./styles/",
        "mainSrc": "main.styl",
        "buildFile": "build.css",
        "shim": [
          "./stylus-shim/nib.js"
        ]
      }
    }
  }
}
